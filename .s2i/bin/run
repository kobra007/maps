#!/bin/bash
cat <<EOF
Start script
-------------------
EOF
echo working on :
pwd

if [   -d "./geoserver_data" ]; then
  
cat <<EOF
=> data dir exist ...
EOF
sudo chmod -R 777 ./geoserver_data

echo --------------
cd ./geoserver_data
ls -l 
echo --------------

fi




exec /wildfly/bin/standalone.sh -b 0.0.0.0 -bmanagement 0.0.0.0
echo Done.


